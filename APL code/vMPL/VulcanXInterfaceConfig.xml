	</filterArmNumerator>
	<filterArmDenominator>
		1, -2.45120315255973, 2.25314883566953, -0.920487588196572, 0.141018879879971
	</filterArmDenominator>
	<filterWristNumerator>
		0.0014048109245751, 0.00561924369830042, 0.00842886554745063, 0.00561924369830042, 0.0014048109245751
	</filterWristNumerator>
