<?xml version="1.0" encoding="utf-8"?>
<vulcanXInterfaceConfig>

  <!-- VulcanX Settings -->
    <!-- Percepts IP (NNN.NNN.NNN.NNN) -->
  	<rightPerceptsIp>
		127.0.0.1
	</rightPerceptsIp>
	<leftPerceptsIp>
		127.0.0.1
	</leftPerceptsIp>

    <!-- Percepts Enable Broadcasting (true/false) -->
	<logRightPercepts>
		true
	</logRightPercepts>	
	<logLeftPercepts>
		true
	</logLeftPercepts>

  <!-- MPL Settings -->
    <!-- MPL Enable - Right/Left (true/false) - make sure that MPLEnableRight is declared first  -->
	<MPLEnableRight>
		true
	</MPLEnableRight>
	<MPLEnableLeft>
		true
	</MPLEnableLeft>

    <!-- MPL Shoulder Location - at start-up (Position (X,Y,Z), floats) -->
	<MPLLocationRight>
		<x_pos>0</x_pos>
		<y_pos>0</y_pos>
		<z_pos>20</z_pos>
	</MPLLocationRight>
	<MPLLocationLeft>
		<x_pos>0</x_pos>
		<y_pos>0</y_pos>
		<z_pos>-20</z_pos>
	</MPLLocationLeft>

    <!-- Show Endpoint Location - red circle (true/false) -->
	<showEndpointRight>
		false
	</showEndpointRight>
	<showEndpointLeft>
		false
	</showEndpointLeft>
	
    <!-- Show Limb Workspace - clear circle (true/false) -->
	<showLimbWorkspaceRight>
		false
	</showLimbWorkspaceRight>
	<showLimbWorkspaceLeft>
		false
	</showLimbWorkspaceLeft>

    <!-- Collisions - Bimanual (true/false) -->
	<bimanualCollisions>
		false
	</bimanualCollisions>
    
    <!-- Collisions Detection Method (default/discrete/continuous/dynamic) -->
	<bimanualCollisionDetectionMethod>
		discrete
	</bimanualCollisionDetectionMethod>

    <!-- Grasp Logic - will assign these values to all objects that contain GraspableObject script -->

      <!-- Grasp Logic - requiresThumbOrPalm (true/false) -->
      <!-- Grasp Logic - requiresThumb (true/false) -->
      <!-- Grasp Logic - requiresPalm (true/false) -->
      <!-- Grasp Logic - requiresFinger (true/false) -->
      <!-- Grasp Logic - requiresNProximalContacts (integer) -->
      <!-- Grasp Logic - requiresNDistalContacts (integer) -->
      <!-- Grasp Logic - requiresNMedialContacts (integer) -->
      <!-- Grasp Logic - requiresContactDirectionality (true/false) -->
	<graspLogic>
		<requiresThumbOrPalm>true</requiresThumbOrPalm>
		<requiresThumb>false</requiresThumb>
		<requiresPalm>false</requiresPalm>
		<requiresFinger>true</requiresFinger>
		<requiresNProximalContacts>0</requiresNProximalContacts>
		<requiresNDistalContacts>0</requiresNDistalContacts>
		<requiresNMedialContacts>0</requiresNMedialContacts>
		<requiresContactDirectionality>true</requiresContactDirectionality>
	</graspLogic>

  <!-- Controller PID Settings -->
